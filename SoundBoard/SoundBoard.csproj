    <ItemGroup>
        <PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="10.0.0-preview.2.25163.2" />
        <PackageReference Include="NAudio" Version="2.2.1" />
        <PackageReference Include="NAudio.Vorbis" Version="1.5.0" />
        <PackageReference Include="Sentry" Version="14.12.1-dump1" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\..\REPO_MenuManager\ModdedMenu\ModdedMenu.csproj" Private="false" />
        <ProjectReference Include="..\..\REPO_Mods\Shared\Shared.csproj" Private="false" />
    </ItemGroup>
    <Import Project="$(ProjectDir)../../REPO_Mods/main.targets" />
    <Target Name="CustomPreBuild" BeforeTargets="Build">
        <Exec Command="$(DotNetToolsDirectory)$(PathSeparator)UnityModPackager --pre" WorkingDirectory="$(ProjectDir)" />
    </Target>
    <Target Name="CustomPostBuild" AfterTargets="Build">
        <Exec Command="$(DotNetToolsDirectory)$(PathSeparator)UnityModPackager --post" WorkingDirectory="$(ProjectDir)" />
        <ItemGroup>
            <AllOutputFiles Include="$(TargetDir)*.dll" />
        </ItemGroup>
        <Copy SourceFiles="@(AllOutputFiles)" DestinationFolder="$(CopyToDirOnBuild)%(RecursiveDir)" OverwriteReadOnlyFiles="true" SkipUnchangedFiles="true" />
    </Target>
    <Import Project="obj/GeneratedResources.targets" />
</Project>